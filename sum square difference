-import java.util.Scanner;
public class Main{
    public static void main(String[] args)
    {
        int sum1=0,sum2=0,n=100,sum3=0,res=0;
        for(int i=0;i<=n;i++)
        {
            sum1=sum1+(i*i);
            sum2=sum2+i;
            sum3=sum2*sum2;
        }
       res=sum3-sum1;
      System.out.println(res);
    }
}
