//Multiples of 3 or 5
public class sumofmultiples {
	public static void main(String[] args)
    {
        int a=1000,i;
        int sum=0;
        for(i=1;i<1000;i++)
        {
            if(i%3==0 || i%5==0)
            {
                sum=sum+i;
            }
        }
        System.out.println("Sum of multiples is " +sum);
    }
}
